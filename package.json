{
  "name": "react-dynamic-components",
  "version": "1.0.12-beta",
  "description": "React library provides multiple types of components (beta version)",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/nachorsanz/react-dynamic-buttons.git"
  },
  "jest": {
    "verbose": true
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:ui-jest": "jest --config=\"./jest.config.ts\" --testPathPattern=\".*ui-jest\\.spec\\..*\" --testEnvironment=\"jsdom\"",
    "test:unit": "jest --config=\"./jest.config.ts\" --testPathPattern=\".*unit\\.spec\\..*\" --env=node",
    "prettier": "npx prettier --write src/",
    "build": "rm -rf dist && npm run build:esm && npm run build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs",
    "build:watch": "tsc --watch",
    "build:watch:esm": "tsc --watch ",
    "build:watch:cjs": "tsc --module commonjs --outDir dist/cjs --watch",
    "dev": "npm run build:watch:esm & npm run build:watch:cjs & npm run storybook",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "predeploy": "npm run test:ui-jest & npm run test:unit & npm run build"
  },
  "author": "Nacho Rodriguez Sanz",
  "license": "MIT",
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "4.7.1",
    "styled-components": "5.3.6"
  },
  "devDependencies": {
    "@babel/core": "7.20.5",
    "@storybook/addon-actions": "6.5.14",
    "@storybook/addon-essentials": "6.5.14",
    "@storybook/addon-interactions": "6.5.14",
    "@storybook/addon-links": "6.5.14",
    "@storybook/builder-webpack5": "6.5.14",
    "@storybook/manager-webpack5": "6.5.14",
    "@storybook/react": "^6.5.14",
    "@storybook/testing-react": "1.3.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/jest": "^29.2.4",
    "@types/node": "18.11.11",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.9",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@typescript-eslint/eslint-plugin": "5.45.1",
    "babel-jest": "^29.3.1",
    "babel-loader": "9.1.0",
    "babel-preset-jest": "^29.2.0",
    "eslint": "8.29.0",
    "eslint-config-standard-with-typescript": "23.0.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-n": "15.6.0",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-react": "7.31.11",
    "eslint-plugin-storybook": "0.6.8",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "jest-transform-stub": "^2.0.0",
    "prettier": "2.8.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "5.3.6",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "4.9.3",
    "webpack": "5.75.0"
  },
  "dependencies": {
    "@emotion/react": "11.10.5",
    "@emotion/styled": "11.10.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "4.7.1",
    "styled-components": "5.3.6"
  },
  "keywords": [
    "react",
    "typescript",
    "buttons",
    "dynamic",
    "react-dynamic-buttons"
  ]
}
